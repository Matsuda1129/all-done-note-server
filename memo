CREATE TEMPORARY TABLE comment_tmp AS SELECT MIN(userId), postId FROM comment GROUP BY postId;
DELETE FROM comment;
INSERT INTO comment SELECT * FROM comment_tmp;
DROP TEMPORARY TABLE comment_tmp;

SELECT DISTINCT userId, postId FROM comments;

